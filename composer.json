{
    "name": "tgalopin/simhash",
    "description": "SimHash similarities algorithm implementation for PHP",
    "keywords": ["algorithm", "simhash", "similarities", "similarity", "difference"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com",
            "homepage": "https://titouangalopin.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SimHash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SimHash\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "symfony/var-dumper": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
